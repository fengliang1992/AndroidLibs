<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fltry.androidlibs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.elastic.ElasticActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <!--协议部分，随便设置-->
                <data
                    android:host="module"
                    android:path="/elastic"
                    android:port="80"
                    android:scheme="fltry" />
                <!--下面这几行也必须得设置-->
                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.refresh.RefreshActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <!--协议部分，随便设置-->
                <data
                    android:host="module"
                    android:path="/refresh"
                    android:port="80"
                    android:scheme="fltry" />
                <!--下面这几行也必须得设置-->
                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.timeselect.TimeSelectActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <!--协议部分，随便设置-->
                <data
                    android:host="module"
                    android:path="/timeSelect"
                    android:port="80"
                    android:scheme="fltry" />
                <!--下面这几行也必须得设置-->
                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.autotext.AutoTextActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <!--协议部分，随便设置-->
                <data
                    android:host="module"
                    android:path="/autoText"
                    android:port="80"
                    android:scheme="fltry" />
                <!--下面这几行也必须得设置-->
                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
    </application>

</manifest>